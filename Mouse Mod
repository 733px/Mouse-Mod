 document.addEventListener("keydown", function(event) {
    if (event.ctrlKey && event.key === "c") {
    document.body.style.cursor = "default";
}
});



 document.addEventListener("keydown", function(event) {
    if (event.ctrlKey && event.key === "x") {
  if (discordButton.style.display === "none") {
      discordButton.style.display = "";
    } else {
      discordButton.style.display = "none";
    }
  if (displayText.style.display === "none") {
      displayText.style.display = "";
    } else {
      displayText.style.display = "none";
    }
  if (moveButton.style.display === "none") {
        moveButton.style.display = "";
      } else {
        moveButton.style.display = "none";
      }
  if (hideButton.style.display === "none") {
        hideButton.style.display = "";
      } else {
        hideButton.style.display = "none";
      }
      if (guiBG.style.display === "none") {
        guiBG.style.display = "";
      } else {
        guiBG.style.display = "none";
      }
      if (allScrollButton.style.display === "none") {
        allScrollButton.style.display = "";
      } else {
        allScrollButton.style.display = "none";
      }
      if (defaultButton.style.display === "none") {
