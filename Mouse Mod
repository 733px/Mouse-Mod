  var isDragging = false;
  var mouseOffset = { x: 0, y: 0 };

  function updateButtonPosition() {
    guiBG.style.left = moveButton.offsetLeft;
    guiBG.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 180) + "px";

    displayText.style.left = moveButton.offsetLeft;
    displayText.style.top = (moveButton.offsetTop + moveButton.offsetHeight - 80) + "px";

    hideButton.style.left = (moveButton.offsetLeft + moveButton.offsetWidth + 10) + "px";
    hideButton.style.top = moveButton.offsetTop;

    defaultButton.style.left = moveButton.offsetLeft;
    defaultButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight - 0) + "px";

    allScrollButton.style.left = moveButton.offsetLeft;
    allScrollButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 40) + "px";

    aliasButton.style.left = moveButton.offsetLeft;
    aliasButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 80) + "px";

    cellButton.style.left = moveButton.offsetLeft;
    cellButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 120) + "px";

    colResizeButton.style.left = moveButton.offsetLeft;
    colResizeButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 160) + "px";

    copyButton.style.left = moveButton.offsetLeft;
    copyButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 200) + "px";

    eResizeButton.style.left = moveButton.offsetLeft;
    eResizeButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 240) + "px";

    grabButton.style.left = moveButton.offsetLeft;
    grabButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 280) + "px";

    helpButton.style.left = moveButton.offsetLeft;
    helpButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 320) + "px";

    progressButton.style.left = moveButton.offsetLeft;
    progressButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 360) + "px";

    notAllowedButton.style.left = moveButton.offsetLeft;
    notAllowedButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 400) + "px";

    discordButton.style.left = moveButton.offsetLeft;
    discordButton.style.top = (moveButton.offsetTop + moveButton.offsetHeight + 440) + "px";

  }









  moveButton.addEventListener("mousedown", function(event) {
    isDragging = true;
    mouseOffset.x = event.clientX - moveButton.offsetLeft;
    mouseOffset.y = event.clientY - moveButton.offsetTop;
  });

  document.addEventListener("mousemove", function(event) {
    if (isDragging) {      
      updateButtonPosition();
      moveButton.style.left = (event.clientX - mouseOffset.x) + "px";
      moveButton.style.top = (event.clientY - mouseOffset.y) + "px";

    }
  });





  document.addEventListener("mouseup", function() {
    isDragging = false;
  });
  setInterval(function() {
updateButtonPosition();
}, 100);
})();


